   "source": [
    "#Write a program using generator to print the even numbers between 0 and n in comma separated form where n is input from console.\n",
    "\n",
    "def even(num):\n",
    "    for i in range(num+1):\n",
    "        if i%2 == 0:\n",
    "            yield i\n",
    "\n",
    "a = int(input())\n",
    "print(*even(a))"
   ]
  }

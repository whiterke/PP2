   "source": [
    "#Write a Python program to calculate the area of regular polygon.\n",
    "import math\n",
    "\n",
    "a = int(input(\"Input number of sides:\"))\n",
    "b = int(input(\"Input the length of a side:\"))\n",
    "c = b/(2 * math.tan(math.pi/a))\n",
    "\n",
    "print(\"The area of the polygon is:\", a*b*c/2)"
   ]
  }

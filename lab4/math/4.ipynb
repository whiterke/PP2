    "#Write a Python program to calculate two date difference in seconds.\n",
    "from datetime import datetime\n",
    "\n",
    "date1 = datetime(2021, 2, 15, 20, 00, 00)\n",
    "date2 = datetime(2021, 2, 17, 21, 00, 00)\n",
    "\n",
    "diff = date2 - date1\n",
    "\n",
    "print(diff.total_seconds())"
   ]
